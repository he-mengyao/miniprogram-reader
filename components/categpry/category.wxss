.title {
  height: 80rpx;
  padding: 0 34rpx;
  color: #888888;
  display: flex;
  align-items: center;
  font-size: 13px;
}
.content {
  display: flex;
  flex-wrap: wrap;
}
.item {
  width: 246rpx;
  height: 100rpx;
  border: 1px solid #eeeeee;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.item:first-child {
  border-left: 0;
}
.item:nth-child(3n){
  border-right: 0;
}
.name {
  font-size: 12px;
}
.count {
  color: #999999;
  font-size: 11px;
}